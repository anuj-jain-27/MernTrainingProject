{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console":"warn",
        "react/react-in-jsx-scope": "off",
        "no-unused-vars": "warn",
        "no-empty":"warn",
        "no-duplicate-case":"warn",
        "no-unreachable-loop":"warn",
        "eqeqeq":"warn",
        "no-alert":"warn",
        "no-extra-semi":"warn"
    }
}
